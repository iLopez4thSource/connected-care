				xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel width="100%" height="100%">
		<g:VerticalPanel width="100%" height="100%">
			<g:Cell verticalAlignment="ALIGN_BOTTOM" height="60px" horizontalAlignment="ALIGN_CENTER">
				<g:Label text="Import External File" styleName="title-label" horizontalAlignment="ALIGN_CENTER"/>
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_CENTER" height="120px" verticalAlignment="ALIGN_MIDDLE">
				<g:HTMLPanel ui:field="uploadFilePanel"/>
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_BOTTOM">
				<b:Well width="50%" size="LARGE">
		<g:Label text="Data Uploaded" styleName="title-label" horizontalAlignment="ALIGN_CENTER"/>
