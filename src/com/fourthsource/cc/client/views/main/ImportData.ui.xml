<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 	xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
				xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel width="100%" height="100%">
		<g:VerticalPanel width="100%" height="100%">
			<g:Cell verticalAlignment="ALIGN_BOTTOM" height="60px" horizontalAlignment="ALIGN_CENTER">
				<g:Label text="Import External File" styleName="title-label" horizontalAlignment="ALIGN_LEFT" width="90%"/>
			</g:Cell>
			<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_CENTER" height="140px">
				<g:HTML width="90%" horizontalAlignment="ALIGN_LEFT" styleName="general-description"><p>To upload your document, please follow the steps below:</p><ul><li><p>Click on the "Choose File" button, select the file you are uploading, and click on "Open". The file name should appear in the textbox to the left of the "Choose File" button.</p></li><li><p>Click on the "Upload" button to start the upload process.</p></li><li><p>Your document will be sent to our document processing servers.&nbsp;</p></li></ul></g:HTML>
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_CENTER" height="120px" verticalAlignment="ALIGN_MIDDLE">
				<g:HTMLPanel ui:field="uploadFilePanel" width="90%"/>
			</g:Cell>
			<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_BOTTOM">
				<b:Well width="90%" size="LARGE">
					<g:Label text="Files Uploaded" styleName="title-label" horizontalAlignment="ALIGN_CENTER"/>
					<g:VerticalPanel ui:field="filesUploadedPanel">
						<b:CellTable ui:field="filesUploadedTable">
    					
 					</b:CellTable>
					</g:VerticalPanel>
				</b:Well>
			</g:Cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 