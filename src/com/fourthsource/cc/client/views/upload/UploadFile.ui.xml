<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<g:DecoratedStackPanel>
			<g:HorizontalPanel width="100%" height="100%" g:StackPanel-text="Upload File">
				<g:Cell width="100%" verticalAlignment="ALIGN_MIDDLE">
					<g:FormPanel ui:field="form" styleName="form-file-upload" action="RESTServices/uploadFile">
						<g:AbsolutePanel width="100%" height="100%" styleName="panel-file-upload" ui:field="absolutePanel">
							<g:TextBox styleName="file" ui:field="textBoxFileUpload" enabled="false" name="fileName"/>
							<g:SimplePanel width="100%" height="100%" styleName="file-upload" ui:field="simplePanel">
								<g:FileUpload ui:field="fileUpload" name="file" width="100%" height="100%"/>
							</g:SimplePanel>
						</g:AbsolutePanel>
					</g:FormPanel>
				</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_CENTER" width="120px" verticalAlignment="ALIGN_MIDDLE">
					<g:Button ui:field="fileUploadButton" text="Add File..." width="100px"/>
				</g:Cell>
			</g:HorizontalPanel> 
		</g:DecoratedStackPanel>
	</g:HTMLPanel>
</ui:UiBinder> 